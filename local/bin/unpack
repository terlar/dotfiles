#!/usr/bin/env bash
main() {
	for file in "$*"; do
		case $file in
			*.tar)
				tar -xf "$file"
				;;
			*.tar.gz|*.tgz)
				tar -zxf "$file"
				;;
			*.tar.bz|*.tar.bz2|*.tbz|*.tbz2)
				tar -jxf "$file"
				;;
			*.gz)
				gunzip -k "$file"
				;;
			*.rar)
				unrar e "$file"
				;;

			*.zip)
				unzip "$file"
				;;
			*.7z)
				7z e "$file"
				;;
			*)
				echo "File $file is of unknown type"
		esac
	done
}

main "$@"
